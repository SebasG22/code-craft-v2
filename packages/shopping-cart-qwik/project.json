{
  "name": "shopping-cart-qwik",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shopping-cart-qwik/src",
  "projectType": "application",
  "targets": {
    "build": {
      "dependsOn": ["build.client", "build.ssr"],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "echo 'finished build'"
        ]
    }
  },

    "build.client": {
      "executor": "@nrwl/vite:build",
      "options": {
        "outputPath": "dist/packages/shopping-car-qwik",
        "configFile": "packages/shopping-car-qwik/vite.config.ts"
      }
    },
    "build.ssr": {
      "executor": "@nrwl/vite:build",
      "defaultConfiguration": "preview",
      "options": {
        "outputPath": "dist/packages/shopping-car-qwik"
      },
      "configurations": {
        "preview": {
          "ssr": "src/entry.preview.tsx",
          "mode": "production"
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "options": {
        "buildTarget": "shopping-cart-qwik:build",
        "port": 4300
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "options": {
        "buildTarget": "shopping-cart-qwik:build.client",
        "mode": "ssr",
        "port": 4200
      }
    },
    "serve.debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --inspect-brk ../../node_modules/vite/bin/vite.js --mode ssr --force",
        "cwd": "packages/shopping-car-qwik"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/shopping-cart-qwik"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/shopping-cart-qwik"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shopping-cart-qwik/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
